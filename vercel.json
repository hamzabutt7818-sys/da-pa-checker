{
"version": 2,
"builds": [
{ "src": "api/.js", "use": "@vercel/node" },
{ "src": "public/**", "use": "@vercel/static" }
],
"rewrites": [
{ "source": "/api/oprank", "destination": "/api/oprank.js" },
{ "source": "/", "destination": "/public/index.html" },
{ "source": "/(.)", "destination": "/public/$1" }
]
}